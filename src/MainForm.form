<?xml version="1.0" encoding="UTF-8"?>
<form xmlns="http://www.intellij.com/uidesigner/form/" version="1" bind-to-class="MainForm">
  <grid id="27dc6" binding="MainPanel" layout-manager="BorderLayout" hgap="10" vgap="5">
    <constraints>
      <xy x="20" y="20" width="500" height="400"/>
    </constraints>
    <properties>
      <background color="-393729"/>
    </properties>
    <border type="none"/>
    <children>
      <component id="4adf6" class="javax.swing.JTextArea" binding="textArea">
        <constraints border-constraint="Center"/>
        <properties/>
      </component>
      <grid id="e2d61" layout-manager="FlowLayout" hgap="10" vgap="5" flow-align="2">
        <constraints border-constraint="South"/>
        <properties/>
        <border type="none"/>
        <children>
          <component id="2cf7c" class="javax.swing.JButton" binding="count_button">
            <constraints/>
            <properties>
              <actionCommand value="count"/>
              <label value="Посчитать"/>
              <maximumSize width="-1" height="-1"/>
              <minimumSize width="-1" height="-1"/>
              <opaque value="true"/>
              <preferredSize width="100" height="30"/>
              <text value="Посчитать"/>
              <toolTipText value="chars counter in text above"/>
            </properties>
          </component>
          <component id="3756b" class="javax.swing.JButton" binding="clear_button">
            <constraints/>
            <properties>
              <actionCommand value="clear"/>
              <label value="Очистить"/>
              <preferredSize width="100" height="30"/>
              <text value="Очистить"/>
              <toolTipText value="clear text above"/>
            </properties>
          </component>
          <vspacer id="a089c">
            <constraints/>
          </vspacer>
        </children>
      </grid>
      <vspacer id="b90bd">
        <constraints border-constraint="East"/>
      </vspacer>
      <vspacer id="8b2f2">
        <constraints border-constraint="West"/>
      </vspacer>
      <hspacer id="31a1d">
        <constraints border-constraint="North"/>
      </hspacer>
    </children>
  </grid>
</form>
